@model IEnumerable<Icarus.Models.vrptCollectible>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IDResident)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Resident)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TerminationDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastPaymentInfo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IDResident)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Resident)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TerminationDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastPaymentInfo)
        </td>
        <td>
           
        </td>
    </tr>
}

</table>
